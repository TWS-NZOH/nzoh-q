# Salesforce Credentials
# Copy this file to .env and fill in your actual credentials
# DO NOT commit .env to git!
# On Azure, these are set via App Service Environment variables (Key Vault references).

SALESFORCE_USERNAME=your_username@example.com
SALESFORCE_PASSWORD=your_password
SALESFORCE_SECURITY_TOKEN=your_security_token

# Optional: live (default) or uat
# SALESFORCE_ENVIRONMENT=live

# Optional (Azure): initials to use when no Windows user (e.g. NZOH_Q_USER_INITIALS=APP)
# NZOH_Q_USER_INITIALS=

# Application Settings
APP_PORT=5000
APP_DEBUG=False
